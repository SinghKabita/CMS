body {
	margin:0px 0px;
	font-family:"Times New Roman", Times, serif;
	background-color:#dee8ff;
}
.testclass
{
    height: 25px;
	background-color:#afc9fe;
	margin: 2px;
	padding-left: 2px;
	padding-right: 2px;
	cursor: pointer;
	border-bottom-style: inset;
	border: #black 1px solid;
	font-size: 11px;
	font-weight:bold;
	color:#516188;
}

.header{
	background:url(../images/header_label.jpg) repeat-x;
	height:110px;
}
.testReport
{
    background-color:Silver;
}
.modalBackground
        {
            background-color: gray;
            filter: alpha(opacity=30);
            opacity: 0.3;
        }
        
.PrioritymodalPopup
{        	
            width:auto;
            height:auto;
            background-color: #a1b7e3;
            color: #4363ae;
            border-color: #000000;
            border: 1px solid;
            cursor: move;
            font-size: medium;            
            clear:both;
            padding:10px 10px 10px 10px;
} 
   
 
.navigation
{
	width:100%;
	background:url(../images/lower_header.jpg) repeat-x;
	height:40px;
	float:right;
	text-align:right;
	padding-bottom:4px;
	vertical-align:middle;
}
.navigation a
{
	color:#002dff;
	font-weight:bold;
	font-family:Arial;
	text-decoration:none;
	font-size:9pt;
	vertical-align:middle;
	padding-bottom:7px;
}

.navigation a:hover
{
	text-decoration:underline;
	color:#002dff;
	font-weight:bold;
	font-family:Arial;
	font-size:9pt;
	vertical-align:middle;
	padding-bottom:7px;
}
.container 
{
	
	float:left;	
	margin: 0px auto;
	padding-bottom: 6px;
	width:982px;
	min-height:575px;
}
.main 
{
	clear: both;
}

.mainContent
{
	margin: auto;
	padding-left:35px;
	background-color:#dee8ff;
	float:left;
	width:95%;
	
	font-size:13px;
	color:#4363ae;
	text-align:center;
	
	
	
}


.maincontentdefault
{
	background-color:#dee8ff;
	width:100%;
	padding:30px 10px 10px 30px;
	text-align:center;
}
		
.content 
{
	float: left;
	width: 75%;
	margin-left:5px;	
}
.othercontent
{
	padding:5px;
}


.sidenav {	
	float: left;
	width: 20%;	
	background-color:#dee8ff;
	
}


/* for login  */



#banner
{
        background:url(../images/lower_header.jpg) repeat-x;
        height:20px;
        color:#369;
}

#login
{
        background :url(../images/login.jpg) no-repeat;
        height:297px;
        background-position:center;
        font-size:8px;
}

.input
 { 
        color :White;
        width:190px;
        font-size:12px;
 }
 
 .input a
 {     color:Black;
       
  }
	
.submit
{
        background:url(../images/submit.jpg);
		height:22px;
		width:67px;
		border:none;
		
}

#footer
{
        clear:both;
        background:url(../images/footer.jpg) repeat-x;
        position:absolute;
        bottom:0;
        height:25px;
        font-size:12px;
        color:#39C;
        text-align:center;
        width:100%;
        color:#ffffff;
}

table.gridtable 
{
	border: 1px solid #789DB3;
	
}

table.gridtable tr
{ 
	border-bottom:1px dashed #789DB3;
}

table.gridtable th 
{
	font-size:14px;
	background-color:#4363ae;
	color:#ffffff;
	vertical-align: middle;
	text-align:center;
	font-weight:bold;
	height:25px; 
}
	
table.gridtable td 
{
	font-size:13px;
	color:#4363ae;
	border-bottom:1px dashed #789DB3;
	padding:4px;
